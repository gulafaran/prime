_EGL_VENDOR_LIBRARY_FILENAMES=/usr/share/glvnd/egl_vendor.d/10_nvidia.json
DXVK_FILTER_DEVICE_NAME=NVIDIA
VKD3D_FILTER_DEVICE_NAME=NVIDIA
